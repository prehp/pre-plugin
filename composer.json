{
    "name": "pre/plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.1",
        "yay/yay": "^0.2"
    },
    "autoload": {
        "files": [
            "src/autoload.php",
            "src/functions.php",
            "src/expanders.php",
            "src/bootstrap.php"
        ],
        "psr-4": {
            "Pre\\Plugin\\": "src"
        }
    },
    "require-dev": {
        "composer/composer": "^1.3",
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Pre\\Plugin\\": "tests"
        }
    },
    "extra": {
        "class": [
            "Pre\\Plugin\\Integration\\Plugin"
        ]
    }
}
